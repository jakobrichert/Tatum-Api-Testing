# Tatum API Configuration
# Copy this file to .env and add your API key

# Get your API key from: https://dashboard.tatum.io/
TATUM_API_KEY=your_api_key_here

# Environment: mainnet or testnet
TATUM_ENVIRONMENT=mainnet

# Default currency for wallet generation
DEFAULT_CURRENCY=BTC

# Default derivation index
DEFAULT_INDEX=0
